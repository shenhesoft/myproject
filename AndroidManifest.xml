<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-sdk
        tools:overrideLibrary="androidx.compose.material3,androidx.compose.material,androidx.compose.material.ripple,androidx.compose.foundation,
        androidx.compose.animation,androidx.compose.foundation.layout,androidx.compose.animation.core,androidx.compose.ui.tooling.data,
        androidx.compose.ui.unit,androidx.compose.ui.geometry,androidx.compose.ui.util,androidx.compose.ui.text,androidx.compose.ui.tooling.preview,
        androidx.compose.ui.tooling,androidx.compose.ui.graphics,androidx.compose.ui.test.manifest,
        androidx.activity.compose,androidx.compose.material.icons,androidx.compose.ui,androidx.graphics.path,androidx.core.ktx,
        androidx.core,androidx.lifecycle.livedata.core,androidx.lifecycle.viewmodel,androidx.lifecycle.process,
        androidx.lifecycle.ktx,androidx.lifecycle.runtime,androidx.lifecycle.viewmodel.ktx,
        androidx.lifecycle.runtime.compose,androidx.lifecycle.viewmodel.savedstate,
        androidx.compose.runtime.saveable,androidx.compose.runtime,androidx.annotation.experimental"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31"
        android:name=".MyApplication">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyApplication">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>